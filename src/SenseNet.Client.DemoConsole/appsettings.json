{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/snclientdemoconsole-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "sensenet": {
    "repository": {
      "url": "https://localhost:44362",
      "authentication": {
        "ClientId": "",
        "ClientSecret": "",
        "ApiKey": ""
      }
    }
  }
}